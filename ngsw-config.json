{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",

  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/assets/site.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },

    {
      "name": "assets",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**/*",
          "/media/**/*",
          "/*.(png|jpg|jpeg|svg|webp|avif|gif|woff|woff2|ttf|otf)"
        ]
      }
    }
  ]
}
