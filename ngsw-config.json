{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/assets/site.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "lazy-bundles",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/chunk-*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**/*",
          "/media/**/*",
          "/*.(png|jpg|jpeg|svg|webp|avif|gif|woff|woff2|ttf|otf)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-cache",
      "urls": [],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "1h",
        "strategy": "freshness"
      }
    }
  ]
}